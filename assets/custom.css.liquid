/**  TYPOGRAPHY    *******************/

#ineveryissue h3 {
  padding:0px;
  margin: 0px;
  margin-top:4px;
  font-size:16px;
  line-height:30px;
}




/* Text Colours *****************************/
.redorange {
  color:#dd390d;
}

.yellow {
  background-color:#dda10d;
}

.white {
  color:#fff;
}

/* Header ********************/

div.header-padding {
  padding: 5px 0;
}

.container.content.header {
  padding:10px 0;
}

.nav ul.search_nav input {
  margin-right:0px;
}

.nav ul li a, .nav ul li a:visited, .nav ul li a:active {
  font-size:0.92em;
}

.logo a {
  width:110px;
  padding-top: 0;
}

.nav ul li a.cart {
  border-radius:0;
}

@media only screen and (max-width: 767px) and (min-width: 480px) {
ul.nav_buttons {
  position: relative;
  top: -39px;
  }

.container.content.header-padding {
  height: 83px;
  overflow: hidden;
}
  
}

@media only screen and (max-width: 479px) {
ul.nav_buttons {
  position: relative;
  top: -39px;
  text-indent: -1000em;
  text-align: left;
}
  
.container.content.header-padding {
  height: 83px;
  overflow: hidden;
}
  
}

/*Footer*/

.footer .container {
  padding:8px;
}

/* FLEX SLIDER */
.flexslider {
  /*max-height:485px;*/
}

/**    SOCIAL BUTTONS *************/

ul.social-buttons {
  list-style:none;
  margin:8px 0 0;
  float:right;
}

.social-buttons li {
  display:inline;
}

a.facebook-button, a.twitter-button, a.google-plus-button {
  color:#fff;
  padding:3px 10px;
  border-radius:3px;
}

a.facebook-button {
  background:#3b5998;
}

a.facebook-button:hover {
  background:#355088;
}

a.twitter-button {
  background:#00aced;
}

a.twitter-button:hover {
  background:#009ad5;
}

a.google-plus-button {
  background:#dd4b39;
}

a.google-plus-button:hover {
  background:#c64333;
}

