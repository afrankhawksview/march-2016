/* Fair Trade Button */

.fair-trade {
  background:#e3e0df;
  text-align:center;
  padding-top:0px;
  padding-bottom:0px;
  margin-bottom:30px;
  color:#585756;
  font-size:16px;

}


/* Monthly Deal */

#monthlydeal {
  -webkit-border-radius: 2px 2px 2px 2px;
  border-radius: 2px 2px 2px 2px;
  -webkit-box-shadow: 0 0 7px 0 rgba(0,0,0,0.3);
  box-shadow: 0 0 7px 0 rgba(0,0,0,0.3);
}


#roaster_logo {
  text-align:center;
}

.all-access {
  margin-top:70px;
  margin-bottom:70px;
}


/* Past Issue */

.past-issue {
  padding-top:24px;
}

/********* Custom Home Slider **************/

.custom_slider {
    background-image: url('slideshow_1.jpg');
    background-size: cover;
    background-position: 50%;
    display: block;
    padding: 68px 4px;
    text-align: center;
}

.custom_slider .headline {
    font-size: 47px;
    margin-bottom: 0.2em;
    line-height: 52px;
    font-weight: normal;
}

.custom_slider .subtitle {
  font-size:28px;
}

.custom_slider .subtitle, .custom_slider .headline {
  text-shadow: 0 1px 2px rgba(0, 0, 0, .7);
}

.custom_slider .action_button {
    margin: 4px;
    padding: 12px 30px;
    font-size: 18px;
}
/********* This Month **********************/

img.this-month-img {
  display:block;
}

h3.this-month {
  text-align:center;
  padding:16px;
  color:#fff;
  background-color:#252016;
  margin-top:0;
}

.coffee-title {
  font-size:14px;
  margin-bottom:12px;
  margin-top:16px;
  line-height:16px;
  min-width:120px;
}


p#quote {
  min-height:56px;
}

.logo-row {
  width:100%;
  padding: 20px 0;
}

.logo-row ul {
  width:100%;
  margin:0;
  list-style-type: none;
  text-align: center;
}


.logo-row li {
  margin: 18px;
  display:inline-block;
  vertical-align:middle;
}

h3.seen-in {
  margin:0;
}

.this-month p {
  font-size:12px;
  line-height:14px;
  margin-bottom:18px;
}

p.this-month-roaster {
  text-align:right;
  margin-bottom:0px;
}

.this-month-roaster:last-of-type {
  margin-bottom:16px;
}

.this-month .swatch-row {
  clear:both;
  margin:16px 0;
}

.this-month .one-third {
  min-width:300px;
  padding:16px 0;
}

.this-month img {
  height:205px;
  margin:0 auto;
  margin-bottom:28px;
}

/* *****In Every Issue ********************* */

.in-every-issue h5 {
  line-height: 1.4em;
  font-size: 1em; 
  text-align: center;
}

h2.in-every-issue {
  background: #281919;
  color: #fff;
  padding: 3px;
  border-radius: 3px;
  margin: 3px 0 18px;
  padding: 0px 0 0 16px;
}

/* ***** Subscription Buttons  ************ */


h3.subscription-price {
  cursor: pointer; 
  color: #fff; 
  margin: 4px 0 0;
}

p.subscription-price {
  position:relative;
  top:-8px;
}

.subscription h2 {
  padding:10px 14px	10px 6px;
  line-height:1.2em;
  background-color:#2e2d2c;
  color:#fff;
}

.subscription h2:hover {
  background-color:#dd390d;
}

.subscription-button {
  padding: 3px 3px 0 12px; 
  margin-bottom:14px; 
  background-color: #dd390d;
  border-radius:3px;
}

.subscription-button:hover {
  background-color:#3d4247;
}



/* ***** Next Delivery Clock ********************* */

.timerbox {
  border: double 6px #281919;
  border-radius:4px;
  height:130px;
}

h2.center.next-delivery {
	margin: 11px auto 0;
}

p.d-h-m {
  margin:0;
}


/* **** Header *************************** */

.menu ul {
	border-radius: 2px;
}


